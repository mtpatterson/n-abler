{
  "name": "n-abler",
  "private": true,
  "version": "1.0.0",
  "description": "n-abler.org main theme",
  "author": "Brandon Patterson",
  "scripts": {
    "browser-sync": "browser-sync start --proxy 'n-abler.localhost' --watch --files '**/*.php, dist/**/*' --no-notify",
    "prefix:css": "postcss dist/*.css --use=autoprefixer --dir=dist",
    "build:css": "sass --style=expanded src/scss:dist",
    "watch:css": "onchange 'src/scss/**/*.scss' -- npm run build:css",
    "webpack": "webpack -d --watch",
    "start": "npm-run-all --parallel browser-sync watch:css webpack",
    "lint": "eslint src"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.10.1",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "core-js": "^3.6.5",
    "eslint": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "postcss": "^7.0.30",
    "postcss-cli": "^7.1.1",
    "sass": "^1.26.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "debug": false,
          "modules": false,
          "loose": true,
          "useBuiltIns": "usage",
          "corejs": "3"
        }
      ],
      "@babel/preset-react"
    ]
  }
}
